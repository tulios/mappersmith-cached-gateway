{
  "name": "mappersmith-cached-gateway",
  "version": "0.3.2",
  "description": "CachedGateway for the Mappersmith project",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/browserify index.js -x mappersmith -s MappersmithCachedGateway -o build/mappersmith-cached-gateway.js",
    "release": "npm run build && ./node_modules/.bin/uglifyjs build/mappersmith-cached-gateway.js -o build/mappersmith-cached-gateway.min.js -c --screw-ie8 --preamble '/*! MappersmithCachedGateway - https://github.com/tulios/mappersmith-cached-gateway - Generated by browserify */'",
    "test-node": "NODE_ENV=test ./node_modules/.bin/mocha test/node/*-test.js",
    "test-browser": "NODE_ENV=test ./node_modules/.bin/karma start karma.conf.js",
    "test": "SINGLE_RUN=true npm run test-browser && npm run test-node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tulios/mappersmith-cached-gateway.git"
  },
  "keywords": [
    "cache",
    "cache store",
    "isomorphic",
    "redis",
    "localstorage",
    "sessionstorage"
  ],
  "author": "Tulio Ornelas <ornelas.tulio@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^8.0.3",
    "browserify-shim": "^3.8.2",
    "chai": "^1.10.0",
    "fakeredis": "^1.0.2",
    "karma": "^0.13.2",
    "karma-browserify": "^4.2.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-commonjs": "^0.0.13",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs2-launcher": "^0.1.6",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^0.3.2",
    "mocha": "^2.0.1",
    "rewire": "^2.3.0",
    "rewireify": "0.0.13",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0",
    "uglify-js": "^2.4.16",
    "aliasify": "^1.7.2"
  },
  "dependencies": {
    "mappersmith": ">=0.11.0 <=1.0.0",
    "redis": "^0.12.1"
  },
  "browserify": {
    "transform": ["browserify-shim"]
  },
  "browserify-shim": "./shim.conf.js"
}
